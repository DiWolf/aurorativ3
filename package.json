{
  "name": "padron-inspectores",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run migrate && node build/app.js",
    "copy:views": "shx mkdir -p build/views && ncp src/views build/views",
    "start:dev": "nodemon -L -r tsconfig-paths/register src/app.ts --exec ts-node-dev --transpileOnly",
    "build": "tsc -p . && npm run copy:views && tsc-alias",
    "build:css": "sass public/admin/scss:public/admin/css public/front/scss:public/front/css --no-source-map",
    "watch:css": "sass --watch public/admin/scss:public/admin/css",
    "migrate": "ts-node src/scripts/migrate.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.858.0",
    "@aws-sdk/s3-request-presigner": "^3.858.0",
    "@azure/msal-node": "^3.6.4",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "i18n": "^0.15.1",
    "method-override": "^3.0.0",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "nunjucks": "^3.2.4",
    "passport": "^0.7.0",
    "slugify": "^1.6.6",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/connect-flash": "^0.0.40",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/i18n": "^0.13.12",
    "@types/jest": "^30.0.0",
    "@types/method-override": "^3.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/nunjucks": "^3.2.6",
    "@types/passport": "^1.0.17",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "ncp": "^2.0.0",
    "nodemon": "^3.1.10",
    "sass": "^1.92.0",
    "shx": "^0.4.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0"
  }
}
